<div class="section default">
    <div class="infobox">
        <h2>{{ aggregationResult.name }}</h2>
        <div class="description">
            {{ aggregationResult.description }}
        </div>
    </div>
    <div class="results">
        <table>
            <thead>
            <tr>
                <td>{{ options.keyName }} {% if count is defined %}(top {{ count }}){% endif %}</td>
                <td>Absolute</td>
                <td>Percent</td>
            </tr>
            </thead>
            <tbody>
            {% for key, value in aggregationResult.results | slice(0, options.limit) %}
                <tr>
                    <td>{{ key | raw }}</td>
                    <td style="width: 50px">{{ value | number_format }}</td>
                    <td style="width: 50px; text-align: right">{{ ((value / websiteCount) * 100) | round(2) | number_format(2) }} %</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>

    <style>
        .fi {
            margin-right: 10px;
        }
    </style>
</div>
